<Window x:Class="WPF_showcase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_showcase"
        mc:Ignorable="d"
        Title="Kalendarz" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition>
                <ColumnDefinition.Width>390</ColumnDefinition.Width>
            </ColumnDefinition>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Viewbox HorizontalAlignment="Left" Margin="10,10,10,10" Grid.Column="0" Grid.Row="0" Grid.RowSpan="3">
            <Calendar HorizontalAlignment="Center"/>
        </Viewbox>
        <TextBlock></TextBlock>
    </Grid>
</Window>
